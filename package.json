{
  "name": "test-heroku",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/app.ts",
    "build": "tsc",
    "postinstall": "npm run build",
    "prod-dbMigrations": "npm run prod-knex:migrate:rollback_all && npm run prod-knex:migrate:latest && npm run prod-knex:seed:run",
    "start": "node dist/src/app.js",
    "knexfile": "knex init -x ts",
    "knex:migrate:make": "npx knex migrate:make --migrations-directory src/database/knexQueryBuilder/migrations --knexfile src/database/knexQueryBuilder/knexfile.ts -x ts",
    "knex:migrate:latest": "npx knex migrate:latest --knexfile src/database/knexQueryBuilder/knexfile.ts",
    "knex:migrate:down": "npx knex migrate:down --knexfile src/database/knexQueryBuilder/knexfile.ts",
    "knex:migrate:rollback_all": "npx knex migrate:rollback --all --knexfile src/database/knexQueryBuilder/knexfile.ts",
    "knex:migrate:rollback": "npx knex migrate:rollback --knexfile src/database/knexQueryBuilder/knexfile.ts",
    "knex:seed:make": "npx knex seed:make -x ts --knexfile src/database/knexQueryBuilder/knexfile.ts",
    "knex:seed:run": "npx knex seed:run --knexfile src/database/knexQueryBuilder/knexfile.ts",
    "prod-knex:migrate:latest": "knex migrate:latest --knexfile dist/src/database/knexQueryBuilder/knexfile.js",
    "prod-knex:seed:run": "knex seed:run --knexfile dist/src/database/knexQueryBuilder/knexfile.js",
    "prod-knex:migrate:rollback_all": "knex migrate:rollback --all --knexfile dist/src/database/knexQueryBuilder/knexfile.js",
    "test": "jest --detectOpenHandles",
    "pretty": "npx prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.11",
    "pg": "^8.7.1",
    "ts-node": "^10.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.10.2",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "jest": "^27.4.0",
    "prettier": "^2.5.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  }
}
